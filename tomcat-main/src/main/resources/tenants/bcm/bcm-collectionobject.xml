<record id="collection-object" is-extension="true">

    <section id="identificationInformation">
        <repeat id="otherNumberList/otherNumber">
            <selector>object-identification-otherNumbers</selector>
            <field id="numberType" in-tab="yes" seperate_ui_container="true"
                   autocomplete="vocab-collectionobjectnumbertype" ui-type="enum">
                <selector>object-identification-other-number-type</selector>
            </field>
        </repeat>

        <repeat id="responsibleDepartments" has-primary="yes">
            <selector>object-identification-responsible-department</selector>
            <field id="responsibleDepartment" ui-search="repeatable" mini="search,list"
                   seperate_ui_container="true" autocomplete="vocab-responsibledepartments" ui-type="enum">
                <selector>object-identification-responsible-department</selector>
            </field>
        </repeat>

        <field id="collection" ui-search="repeatable" seperate_ui_container="true" autocomplete="vocab-collection"
               ui-type="enum">
            <selector>object-identification-collection</selector>
        </field>

        <field id="recordStatus" ui-search="repeatable" seperate_ui_container="true" autocomplete="vocab-recordstatus"
               ui-type="enum">
            <selector>object-identification-record-status</selector>
        </field>

        <repeat id="titleGroupList/titleGroup">
            <field id="titleType" seperate_ui_container="true" autocomplete="vocab-titletype" ui-type="enum">
                <selector>object-identification-object-title-type</selector>
            </field>
        </repeat>


        <repeat id="objectNameList/objectNameGroup">

            <field id="objectNameCurrency" seperate_ui_container="true" autocomplete="vocab-objectnamecurrency"
                   ui-type="enum">
                <selector>object-identification-object-currency</selector>
            </field>

            <field id="objectNameLevel" seperate_ui_container="true" autocomplete="vocab-objectnamelevel"
                   ui-type="enum">
                <selector>object-identification-object-level</selector>
            </field>

            <field id="objectNameSystem" seperate_ui_container="true" autocomplete="vocab-objectnamesystem"
                   ui-type="enum">
                <selector>object-identification-object-system</selector>
            </field>

            <field id="objectNameType" seperate_ui_container="true" autocomplete="vocab-objectnametype" ui-type="enum">
                <selector>object-identification-object-type</selector>
            </field>
        </repeat>

    </section>


    <section id="descriptionInformation">
        <repeat id="objectStatusList" services-type-anonymous="false">
            <field id="objectStatus" ui-search="repeatable" seperate_ui_container="true"
                   autocomplete="vocab-objectstatus" ui-type="enum">
                <selector>object-description-object-status</selector>
            </field>
        </repeat>

        <field id="sex" ui-search="repeatable" seperate_ui_container="true" autocomplete="vocab-sex" ui-type="enum">
            <selector>object-description-sex</selector>
        </field>

        <field id="phase" ui-search="repeatable" seperate_ui_container="true" autocomplete="vocab-phase" ui-type="enum">
            <selector>object-description-phase</selector>
        </field>

        <repeat id="forms">
            <field id="form" ui-search="repeatable" seperate_ui_container="true" autocomplete="vocab-form"
                   ui-type="enum">
                <selector>object-description-form</selector>
            </field>
        </repeat>

        <field id="ageUnit" seperate_ui_container="true" autocomplete="vocab-ageunit" ui-type="enum">
            <selector>object-description-age-unit</selector>
        </field>

        <repeat id="objectComponentGroupList/objectComponentGroup">
            <!-- selector>collection-object-objectComponentGroup</selector -->
            <field id="objectComponentName" ui-search="repeatable" seperate_ui_container="true"
                   autocomplete="vocab-objectcomponentname" ui-type="enum">
                <selector>object-description-object-component-name</selector>
            </field>
        </repeat>

        <repeat id="technicalAttributeGroupList/technicalAttributeGroup">
            <field id="technicalAttribute" ui-search="repeatable" seperate_ui_container="true"
                   autocomplete="vocab-technicalattribute" ui-type="enum">
                <selector>object-description-technical-attribute</selector>
            </field>
            <field id="technicalAttributeMeasurement" seperate_ui_container="true"
                   autocomplete="vocab-technicalattributemeasurement" ui-type="enum">
                <selector>object-description-technical-attribute-measurement</selector>
            </field>
            <field id="technicalAttributeMeasurementUnit" seperate_ui_container="true"
                   autocomplete="vocab-technicalattributemeasurementunit" ui-type="enum">
                <selector>object-description-technical-attribute-unit</selector>
            </field>
        </repeat>

        <section id="contentInformation">
            <repeat id="contentPositions">
                <field id="contentPosition" seperate_ui_container="true" autocomplete="vocab-contentpositions"
                       ui-type="enum">
                    <selector>object-description-content-position</selector>
                </field>
            </repeat>

            <repeat id="contentObjectGroupList/contentObjectGroup">
                <field id="contentObjectType" seperate_ui_container="true" autocomplete="vocab-contentobjecttype"
                       ui-type="enum">
                    <selector>object-description-content-object-type</selector>
                </field>
            </repeat>

            <repeat id="contentScripts">
                <field id="contentScript" seperate_ui_container="true" autocomplete="vocab-contentscript">
                    <selector>object-description-content-script</selector>
                </field>
            </repeat>
        </section>

        <section id="textualInscriptionInformation">
            <repeat id="textualInscriptionGroupList/textualInscriptionGroup">
                <field id="inscriptionContentPosition" seperate_ui_container="true"
                       autocomplete="vocab-inscriptioncontentposition" ui-type="enum">
                    <selector>object-description-inscription-content-position</selector>
                </field>

                <field id="inscriptionContentScript" seperate_ui_container="true"
                       autocomplete="vocab-inscriptioncontentscript" ui-type="enum">
                    <selector>object-description-inscription-content-script</selector>
                </field>

                <field id="inscriptionContentType" seperate_ui_container="true"
                       autocomplete="vocab-inscriptioncontenttype"
                       ui-type="enum">
                    <selector>object-description-inscription-content-type</selector>
                </field>
            </repeat>
        </section>


        <section id="nonTextualInscriptionInformation">
            <repeat id="nonTextualInscriptionGroupList/nonTextualInscriptionGroup">

                <field id="inscriptionDescriptionPosition" seperate_ui_container="true"
                       autocomplete="vocab-inscriptiondescriptionposition" ui-type="enum">
                    <selector>object-description-inscription-description-position</selector>
                </field>

                <field id="inscriptionDescriptionType" seperate_ui_container="true"
                       autocomplete="vocab-inscriptiondescriptiontype" ui-type="enum">
                    <selector>object-description-inscription-description-type</selector>
                </field>
            </repeat>
        </section>
    </section>

    <field id="ownershipAccess" seperate_ui_container="true" autocomplete="vocab-ownershipaccess" ui-type="enum">
        <selector>object-history-association-access</selector>
    </field>

    <field id="ownershipCategory" seperate_ui_container="true" autocomplete="vocab-ownershipcategory" ui-type="enum">
        <selector>object-history-association-category</selector>
    </field>

    <field id="ownershipExchangeMethod" seperate_ui_container="true" autocomplete="vocab-ownershipexchangemethod"
           ui-type="enum">
        <selector>object-history-association-exchange-method</selector>
    </field>

</record>