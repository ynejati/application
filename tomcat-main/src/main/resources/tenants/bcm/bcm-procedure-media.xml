<record id="media" is-extension="true">

    <services-record-path id="bcm">media_bcm:http://collectionspace.org/services/media/local/bcm,media_bcm
    </services-record-path>
    <section id="mediaInformation">
        <repeat id="types">
            <services-tag>typeList</services-tag>
            <field id="type" ui-search="repeatable" autocomplete="vocab-mediatypes"
                   ui-type="enum">>
                <enum>
                    <blank-value>Please select a value</blank-value>
                </enum>
            </field>
        </repeat>
    </section>
</record>